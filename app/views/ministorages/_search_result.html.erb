<% if ministorages.length > 0 %>
	<table width="100%" cellpadding="0" cellspacing="0" class="mns-result">
		<tr>
			<th>迷你倉</th>
			<th>地址</th>
			<th>電話</th>
		</tr>
		<% ministorages.each do |ministorage|%>
			<tr>
				<td><%= link_to ministorage.name , ministorage_path(ministorage), { class: 'siteName' }%></td>
			    <td><%= ministorage.address %></td>
			    <td><%= ministorage.phone %></td>
			</tr>        
		<% end %>
	</table>      
<% else %>
	<!-- <h3>利用此條件 查無站點 請重新查詢</h3> -->
	<h3><%= @target_city_name if @target_city_name %> <%= @target_district_name if @target_district_name %> 查無站點 請重新查詢</h3>
<% end %>